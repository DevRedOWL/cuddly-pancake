<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/call_stats_audio" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_audio"/>
        <TableLayout android:layout_gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/callStatsAudio" android:paddingLeft="5dp" android:paddingRight="5dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/codec_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/call_stats_codec"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/encoder_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_encoder_name"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/decoder_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_decoder_name"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/uploadBandwith_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_upload"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/downloadBandwith_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_download"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/ice_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_ice"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/ip_audio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_ip"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/senderLossRateAudio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_sender_loss_rate"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/receiverLossRateAudio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_receiver_loss_rate"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/jitterBufferAudio" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_jitter_buffer"/>
        </TableLayout>
        <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/call_stats_video" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_video"/>
        <TableLayout android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/callStatsVideo" android:paddingLeft="5dp" android:paddingRight="5dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/codec_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_codec"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/encoder_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_encoder_name"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/decoder_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_decoder_name"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/uploadBandwith_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_upload"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/downloadBandwith_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_download"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/estimatedDownloadBandwidth_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_estimated_download"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/ice_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_ice"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/ip_video" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_ip"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/senderLossRateVideo" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_sender_loss_rate"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/receiverLossRateVideo" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_receiver_loss_rate"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/video_resolution_sent" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_video_resolution_sent"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/video_resolution_received" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_video_resolution_received"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/video_fps_sent" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_video_fps_sent"/>
            <TextView android:textSize="12sp" android:textColor="@color/colorB" android:gravity="center" android:id="@+id/video_fps_received" android:paddingLeft="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/call_stats_video_fps_received"/>
        </TableLayout>
    </LinearLayout>
</ScrollView>
